{
    "name": "flowerallure/frame-design",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Core\\": "core/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "authors": [
        {
            "name": "flowerallure",
            "email": "floweralluer@example.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "ext-pdo": "*",
        "php": "^8.2",
        "psr/container": "^2.0",
        "psr/http-message": "^1.1",
        "twig/twig": "^3.7"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.21",
        "symfony/var-dumper": "^6.3"
    },
    "scripts": {
        "php-cs-fixer": "vendor/bin/php-cs-fixer fix --config=php-cs-fixer.php --using-cache=no --diff --allow-risky=yes --ansi",
        "dry-php-cs-fixer": "vendor/bin/php-cs-fixer fix --config=php-cs-fixer.php --using-cache=no --dry-run --diff --allow-risky=yes --ansi"
    }
}
